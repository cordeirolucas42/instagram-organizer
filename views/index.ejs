<% include partials/header%>

<div class="ui container">
	<div class="ui raised segment">
		<div class="ui form">
			<h1 class="ui header field">
				Organize Your Photos
			</h1>
			<a href="/photos/new" class="ui primary button field">
				Add Photo
			</a>
			<div class="ui three column grid field">
				<%photos.forEach((photo)=>{%>
					<div class="column">
						<img class="ui medium rounded bordered image" src="<%=photo.source%>" height="400px">
					</div>
				<%})%>
			</div>	
		</div>
	</div>	
</div>



<% include partials/footer%>